{
  "extends": [
    "react-app",
    "plugin:prettier/recommended",
    "plugin:jest/recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["jsx-a11y"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react/display-name": "on",
    "react/prop-types": "on",
    "react/react-in-jsx-scope": "off"
  }
}
